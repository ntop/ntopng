{
  "name": "ntopng",
  "version": "5.3.0",
  "description": "ntopng project",
  "main": "http_src/ntopng.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "css:lint": "stylelint './http_src/**/*.scss' || true",
    "js:lint": "eslint './http_src/**/*.js'",
    "watch:css": "onchange './http_src/**/*.scss' -- npm run css:lint && webpack --mode=development --config webpack.prod.js",
    "watch:js": "onchange './http_src/**/*.js' -- webpack --mode=development --config webpack.prod.js",
    "watch": "run-p watch:*",
    "build": "npm run css:lint && webpack --mode=production --config webpack.prod.js; cp -R httpdocs/tmp-dist/* httpdocs/dist",
    "build:dev": "npm run css:lint && webpack --mode=development --config webpack.prod.js; cp -R httpdocs/tmp-dist/* httpdocs/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntop/ntopng.git"
  },
